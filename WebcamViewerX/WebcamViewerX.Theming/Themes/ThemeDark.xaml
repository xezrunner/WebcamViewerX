<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="AccentColor" Color="#008ADC" />
    
    <SolidColorBrush x:Key="Background" Color="#1A1A1A" />
    <SolidColorBrush x:Key="Foreground" Color="White" />

    <!--#region Foreground secondaries -->
    <SolidColorBrush x:Key="Foreground_Secondary" Color="#D0D0D0" />

    <SolidColorBrush x:Key="Foreground_Titlebar" Color="#FFFFFF" />
    <!--#endregion-->

    <!--#region Background secondaries -->
    <SolidColorBrush x:Key="Background_Secondary" Color="#222222" />
    <!--#endregion-->

    <!--#region Home -->
    <SolidColorBrush x:Key="menu_Background" Color="#8F000000" />
    <!--#endregion-->

</ResourceDictionary>